@article{14,
  title = {Time-Series Analyses of Air Pollution and Mortality in the United States: A Subsampling Approach},
  author = {Moolgavkar, Suresh H. and McClellan, Roger O. and Dewanji, Anup and Turim, Jay and Luebeck, E. Georg and Edwards, Melanie},
  year = {2013},
